<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/Iniciar_S"
    tools:context=".Modificar_Cuenta">

    <Button
        android:id="@+id/button9"
        android:layout_width="219dp"
        android:layout_height="87dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/Rectángulo16"
        android:text="NO"
        android:textColor="@color/mr_cast_meta_default_text_color"
        android:textSize="42sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="316dp"
        android:layout_height="141dp"
        android:text="¿Estas seguro que quieres eliminar esta cuenta?"
        android:textColor="@color/mr_cast_meta_default_text_color"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="210dp" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="293dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:srcCompat="@drawable/Trazado1"
        tools:layout_editor_absoluteX="118dp" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="62dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView26"
        app:srcCompat="@drawable/Trazado2" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="471dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/Rectángulo9" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/LogoHotel" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="293dp"
        android:layout_height="68dp"
        android:layout_marginBottom="517dp"
        android:text="Eliminar Cuenta"
        android:textColor="@color/mr_cast_meta_default_text_color"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.228"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:layout_constraintVertical_bias="0.333" />

    <Button
        android:id="@+id/button8"
        android:layout_width="219dp"
        android:layout_height="87dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/Rectángulo16"
        android:paddingLeft="35dp"
        android:text="SI"
        android:textAlignment="textStart"
        android:textColor="@color/mr_cast_meta_default_text_color"
        android:textSize="42sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/Imagen3"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="387dp" />

</androidx.constraintlayout.widget.ConstraintLayout>