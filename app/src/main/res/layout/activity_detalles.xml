<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/Iniciar_S"
    tools:context=".Modificar_Cuenta">

    <TextView
        android:id="@+id/fecha_salida"
        android:layout_width="370dp"
        android:layout_height="132dp"
        android:layout_marginTop="8dp"
        android:text="Fecha de salida:"
        android:textColor="@color/mr_cast_meta_default_text_color"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fecha_llegada" />

    <TextView
        android:id="@+id/fecha_llegada"
        android:layout_width="370dp"
        android:layout_height="132dp"
        android:layout_marginTop="16dp"
        android:text="@string/fecha_llegada"
        android:textColor="@color/mr_cast_meta_default_text_color"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/a_nombre" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="293dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/barra_superior"
        app:srcCompat="@drawable/Trazado1" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="62dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView26"
        app:srcCompat="@drawable/Trazado2" />

    <ImageView
        android:id="@+id/barra_superior"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="471dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/Rectángulo9" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/LogoHotel" />

    <TextView
        android:id="@+id/elim_cta"
        android:layout_width="401dp"
        android:layout_height="90dp"
        android:layout_marginBottom="517dp"
        android:text="@string/detalles"
        android:textColor="@color/mr_cast_meta_default_text_color"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.228"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/barra_superior"
        app:layout_constraintVertical_bias="0.333" />

    <TextView
        android:id="@+id/a_nombre"
        android:layout_width="329dp"
        android:layout_height="56dp"
        android:layout_marginTop="14dp"
        android:text="@string/a_nombre"
        android:textColor="@color/mr_cast_meta_default_text_color"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/elim_cta" />

    <Button
        android:id="@+id/btn_confirmar"
        android:layout_width="271dp"
        android:layout_height="116dp"
        android:layout_marginBottom="84dp"
        android:background="@drawable/Rectángulo18"
        android:text="@string/expandir"
        android:textColor="@color/mr_cast_meta_default_text_color"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.471"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>